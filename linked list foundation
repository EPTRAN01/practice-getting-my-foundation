class Node: 
  def __init__(self, value):
    self.value = value
    self.next = None

class Linkedlist:
  def __init__(self):
    self.head = None 
  def append(self, value):
    new_node = Node(value)
    if self.head is None:
      self.head = new_node
      return
    current = self.head
    while current.next is not None:
        current = current.next
    current.next = new_node
      




      def reverse(self):
        prev = None
        current = self.head

        while current is not None:
            next_node = current.next
            current.next = prev
            prev = current
            current = next_node

        self.head = prev


      def cyclechecker(head):
          slow = head
          fast = head

          while fast and fast.next:
            slow = slow.next
            fast = fast.next.next
            if slow == fast:
              return True
            return False
            
