class node: 
  def __init__(self, value):
    self.value = value
    self.next = None

class linkedlist:
  def __init__(self):
    self.head = None 
  def append(self, value):
    new_node = node(value)
    if self.head is None:
      self.head = new_node
      return
    current = self.head
    while True:
      if current.next is None:
        current.next = new_node
        break
      current = current.next
