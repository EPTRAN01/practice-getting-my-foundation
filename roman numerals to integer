class Solution(object):
    def romanToInt(self, s):
        index = {
            "I": 1, "V": 5, "X": 10,
            "L": 50, "C": 100,
            "D": 500, "M": 1000
        }
        i = 0
        total = 0
        while i < len(s):
            # Check for subtractive case
            if i + 1 < len(s) and index[s[i]] < index[s[i + 1]]:
                total += index[s[i + 1]] - index[s[i]]
                i += 2  # skip both characters
            else:
                total += index[s[i]]
                i += 1  # move to the next character

        return total
